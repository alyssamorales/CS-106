/**
 * 
 */
import java.io.*;
import java.util.*;

/**
 * @author amorales
 *
 */
public class FileLookahead {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
			throws FileNotFoundException {
				Scanner input = new Scanner (new File("src/input.txt"));
			double obcheckings = 1540.39;
			double obsavings = 1000.00;
			double obcreditcard = 549.20;
			
			while (input.hasNextDouble()); {
				obcheckings += input.nextDouble ();
				input.next();	
			}
			System.out.println("Checkings Ending Balance: ");
			System.out.println("Savings Ending Balance: ");
			System.out.println("Credit Card Ending Balance: ");
			
		}
	}
