
public class Match {
	String date;
	String versus;
	String score;
	String type;
public Match (String initdate, String initversus, String initscore, String inittype) {
	date = initdate;
	versus = initversus;
	score = initscore;
	type = inittype;
}

public String getDate() {
	return this.date;
}

public String getVersus() {
	return this.versus;
}
}
